{
  "env": {
    "node": true,
    "jest": true,
    "es2021": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
  "no-console": ["warn", { "allow": ["warn", "error", "debug", "log"] }],
    "semi": ["error", "always"]
  }
}
